{
    "name": "flutter_docker",
    "dockerFile": "Dockerfile",
    "context": "..",
    "settings": {
      "terminal.integrated.shell.linux": null
    },
    "runArgs": ["--privileged"],
    "extensions": ["dart-code.flutter"],
    "workspaceMount": "source=${localWorkspaceFolder}/laps,target=/workspace,type=bind,consistency=delegated",
    "workspaceFolder": "/workspace"
}